digraph G {
	rankdir=LR;
	node [shape="record"];

	boot_class_tab [
		label = "\
			<this>Boot Class Hash Table|\
			<tab>HashEntry *hash_table;|\
			int hash_size;|\
			int hash_count;|\
			VMLock lock;
		"
	];

	table [
		label = "\
			{<table>hash|void *data}|\
			{hash|void *data}|\
			{hash|void  *data}
		"
	];

	boot_class_tab:tab -> table:table;
}
