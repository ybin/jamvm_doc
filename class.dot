digraph G {
	rankdir=LR;
	node [shape="record"];

	boot_class_tab [
		label = "\
			<this>Boot Class Hash Table|\
			<tab>HashEntry *hash_table;|\
			int hash_size;|\
			int hash_count;|\
			VMLock lock;
		"
	];

	table [
		label = "\
			<table>......|\
			{hash(classname)|<data>void *data}|\
			{hash(classname)|void *data}|\
			{hash(classname)|void *data}|\
			......
		"
	];

	class [
		label = "\
			<this>Class|\
			{{uintptr_t lock;|Class *class;}|class\nheader}|\
			{{
			a|b
			}|class\nblock}
		"
	]

	boot_class_tab:tab -> table:table;
	table:data -> class:this;
}
